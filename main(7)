class Animal:
    _count = 0
    
    def __init__(self):
        Animal._count += 1

    def get_count():
        return Animal._count
    
class Dog(Animal):
    def voice(self):
        return "Гав"
        
class Cat(Animal):
    def voice(self):
        return "Мяу"
        
class Cow(Animal):
    def voice(self):
        return "Муу"
        
dog = Dog()
cat = Cat()
cow = Cow()

print(dog.voice())
print (cat.voice())
print (cow.voice())

print(f"Количество созданных экземпляров: {Animal.get_count()}")
